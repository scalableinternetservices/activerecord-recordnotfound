# Ignore bundler config
/.bundle

# Ignore temporary files and folders generated by Rails
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore cache and socket files
/tmp/pids
/tmp/cache
/tmp/sockets

# Ignore compiled files from Bootsnap (speeds up Rails boot time)
tmp/cache/bootsnap/compile-cache/

# Ignore Byebug command history file.
.byebug_history

# Ignore SQLite database files
*.sqlite3
*.sqlite3-journal

# Ignore Node.js modules and Yarn cache
/node_modules
/yarn-error.log

# Ignore dotenv files (only if using environment variables this way)
.env
.env.*.local
*.key

# Ignore Spring server files
/spring/*.pid
/spring/*.sock

# Ignore uploaded files in Active Storage (if you're using Active Storage)
storage/*

# Ignore public uploads
/public/uploads

# Ignore assets generated by webpacker
/public/packs
/public/packs-test
/node_modules/

# Ignore specific IDE and OS files
*.swp
*~
.project
.DS_Store
.idea/
.vscode/

# Ignore coverage reports
/coverage/

